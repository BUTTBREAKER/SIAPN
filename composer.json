{
  "require": {
    "php": ">=8.2",
    "flightphp/core": "^3.13",
    "illuminate/container": "^11.30"
  },
  "autoload": {
    "psr-4": {
      "SIPAN\\": "app"
    }
  },
  "scripts": {
    "dev": [
      "Composer\\Config::disableProcessTimeout",
      "composer setup",
      "php -S localhost:80"
    ],
    "setup": "php -r \"if (!file_exists('.env.php')) copy('.env.example.php', '.env.php');\""
  },
  "require-dev": {
    "symfony/var-dumper": "^7.1"
  }
}
